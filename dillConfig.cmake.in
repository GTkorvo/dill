# - Config file for the Dill package
# It defines the following variables
#  DILL_INCLUDE_DIRS - include directories for Dill
#  DILL_LIBRARIES    - libraries to link against
#  DILL_EXECUTABLE   - the bar executable
 
# Compute paths
get_filename_component(DILL_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)
set(DILL_INCLUDE_DIRS "@CONF_INCLUDE_DIRS@")
 
# Our library dependencies (contains definitions for IMPORTED targets)
if(NOT TARGET dill AND NOT dill_BINARY_DIR)
  include("${DILL_CMAKE_DIR}/dillTargets.cmake")
endif()
 
# These are IMPORTED targets created by DillTargets.cmake
set(DILL_LIBRARIES dill)
